    "java.refactor.renameFromFileExplorer": "autoApply",
    "editor.columnSelection": false,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true, // enable semantic highlighting for all themes
        "rules": {
            // types are BOLD
            // statics are ITALIC
            // non final variables are UNDERLINED
            "type": {
                "bold": true,
            },
            "*.static": {
                "italic": true,
            },
            "variable": {
                "underline": true,
            //    "foreground": "#ff0000C3"
            },
            "*.readonly": {
                "italic" : false,
                "underline": false,
                //"foreground": "#000000C3"
            },
        }
    },
    //"haskell.semanticHighlighting.enabled": true,
    "markdown.preview.fontSize": 12,
    "editor.fontSize": 18,
    "workbench.startupEditor": "newUntitledFile",
    "editor.semanticHighlighting.enabled": false,
    "editor.occurrencesHighlight": false,
    "java.semanticHighlighting.enabled": true,
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.fontSize": 14,
    "java.import.gradle.arguments": "",
    "java.import.gradle.jvmArguments": "",
    "java.import.gradle.user.home": "",
    "java.import.gradle.version": "",
